@{
    Layout = "_PartialModel";
}

@section Content 
{
    <div class="card-body p-0">
        <table class="table table-hover mb-0">
            <thead>
            <tr>
                @RenderSection("ModelCols", required: true)
            </tr>
            </thead>
            <tbody class="align-middle">
            @RenderSection("ModelRows", required: true)
            </tbody>
        </table>
    </div>
}

@section Footer 
{
    <a asp-action="Create">@ViewData["TitleCreate"]</a>
}